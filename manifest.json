{
  "manifest_version": 2,

  "name": "Betsson Tool",
  "description": "Wala lang extension",
  "version": "0.1",

  "permissions": [
	"https://*/*",
	"http://*/*",
    "https://sportsbook.betssoncom.local/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },  
  "content_scripts": [
    {
      "matches": ["https://www.google.com.ph/*","https://*/*"],
      "js": ["jquery-1.10.2.min.js"]
    }
  ]
}